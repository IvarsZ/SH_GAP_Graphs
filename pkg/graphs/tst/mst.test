testMST := function(graph, expectedMSTWeight)
  local edges, edge, actualMSTWeight;

  edges := MinimumSpanningTree(graph);

  actualMSTWeight := 0;
  for edge in edges do
    actualMSTWeight := actualMSTWeight + GetWeightedEdge(graph, edge[1], edge[2]);
  od;

  if actualMSTWeight = expectedMSTWeight then
    Print("mst test successful\n");
  else  
    Print("Error in mstp test\n");
    Print("expected weight: ", expectedMSTWeight, "\n");
    Print("actual weight: ", actualMSTWeight, "\n");
    Print("mst edges: ", edges, "\n");
    Print("graph: ", graph!.successors);
    Print("weights: ", graph!.weights);
  fi;
end;

testMSTPrims := function(graph, expectedMSTWeight)
  local edges, edge, actualMSTWeight;

  edges := MinimumSpanningTreePrims(graph);

  actualMSTWeight := 0;
  for edge in edges do
    actualMSTWeight := actualMSTWeight + GetWeightedEdge(graph, edge[1], edge[2]);
  od;

  if actualMSTWeight = expectedMSTWeight then
    Print("mst prims test successful\n");
  else  
    Print("Error in mst prims test\n");
    Print("expected weight: ", expectedMSTWeight, "\n");
    Print("actual weight: ", actualMSTWeight, "\n");
    Print("mst edges: ", edges, "\n");
    Print("graph: ", graph!.successors);
    Print("weights: ", graph!.weights);
  fi;
end;

